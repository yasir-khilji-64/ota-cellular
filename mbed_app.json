{
    "config": {
        "debug": {
            "help": "Enbale/Disable logger",
            "value": true
        },
        "version": {
            "help": "Current firmware version",
            "value": "\"0.1.0\""
        }
    },
    "target_overrides": {
        "*": {
            "platform.stdio-convert-newlines": true,
            "platform.stdio-baud-rate": 115200,
